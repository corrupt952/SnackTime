{
  "name": "snack_time",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.15.0",
  "version": "2025.06.4",
  "description": "This extension is a timer on the website to help with timekeeping for meetings",
  "scripts": {
    "test": "vitest",
    "pretest:e2e": "pnpm build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rm -rf dist",
    "dev": "pnpm run clean && vite",
    "build": "pnpm run clean && tsc && vite build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "STORYBOOK_BUILD=true storybook build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\""
  },
  "author": "K@zuki.",
  "license": "MIT",
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.2",
    "@playwright/test": "^1.53.1",
    "@storybook/addon-docs": "^9.0.12",
    "@storybook/addon-onboarding": "^9.0.12",
    "@storybook/react-vite": "^9.0.12",
    "@tailwindcss/vite": "^4.1.10",
    "@types/chrome": "^0.0.279",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-react": "^4.5.2",
    "glob": "^11.0.0",
    "lucide-react": "^0.542.0",
    "playwright": "^1.53.1",
    "prettier": "^3.5.3",
    "storybook": "^9.0.12",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.0.4",
    "vite": "^7.0.0",
    "vitest": "^3.0.4"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "js-yaml": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.0"
  }
}
